CONFIG_ENV=Profiles/clover_lineage_a15.env

DEVICE_NAME=MiPad4
DEVICE_CODENAME=clover

CUSTOM_CMDS:CLANG_TRIPLE=aarch64-linux-gnu- vendor/a42xq_eur_open_defconfig
EXTRA_CMDS:"DTC_EXT=tools/dtc CONFIG_BUILD_ARM64_DT_OVERLAY=y"
EXTRA_CMDS:PYTHON=/usr/bin/python2

KERNEL_SOURCE=https://github.com/mads2570/a42-kernel.git
KERNEL_BRANCH=main
CLANG_SOURCE=https://github.com/SN-Abdullah-Al-Noman/llvm-arm-toolchain-ship-10.0.git
CLANG_BRANCH=main

GCC_64_SOURCE=https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9
GCC_64_BRANCH=android12L-release
GCC_32_SOURCE=https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/x86/x86_64-linux-android-4.9
GCC_32_BRANCH=android12L-release

DEFCONFIG_SOURCE=
DEFCONFIG_NAME=vendor/a42xq_eur_open_defconfig


KERNELSU_SOURCE=https://raw.githubusercontent.com/ShirkNeko/SukiSU-Ultra/main/kernel/setup.sh
KERNELSU_BRANCH=susfs-dev
KERNELSU_NAME=KernelSU

SUSFS_ENABLE=true
SUSFS_FIXED=true
SUSFS_UPDATE=true

AK3_SOURCE=https://github.com/osm0sis/AnyKernel3.git
AK3_BRANCH=master

BOOT_SOURCE=https://github.com/mads2570/a42xq_boot.img/raw/master/boot.img

LXC_ENABLE=false

HAVE_NO_DTBO=false
HAVE_NO_DTBO_TOOL=false

ROM_TEXT=lineageOS_android15
